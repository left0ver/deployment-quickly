{
  "name": "ldq-cli",
  "version": "0.0.5",
  "description": "node实现自动化部署项目至服务器",
  "keywords": [
    "ldq-cli",
    "cli",
    "ldq",
    "dq"
  ],
  "main": "index.js",
  "bin": {
    "dq": "./index.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "pb": "npm publish",
    "tag": "git tag -a v%npm_package_version% && git push origin v%npm_package_version%",
    "release": "npm run pb && npm run tag",
    "lint": "eslint ./ --fix",
    "prepare": "husky install",
    "cm": "npx cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/left0ver/dq-cli.git"
  },
  "author": {
    "name": "left0ver",
    "url": "https://github.com/left0ver"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/left0ver/dq-cli/issues",
    "email": "1614674987@qq.com"
  },
  "homepage": "https://github.com/left0ver/dq-cli#readme",
  "dependencies": {
    "commander": "^9.2.0",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.2",
    "node-ssh": "^12.0.4",
    "ora": "^6.1.0"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.14.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
